{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/jsx-one-expression-per-line": [ 2, "never" ],
    "react/prop-types": "warn",
    "semi": [2, "never"],
    "comma-dangle": ["error", "always-multiline"]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true
  }
}