{
  "parser": "babel-eslint",
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "plugins": ["react-hooks"],
  "rules": {
    "prettier/prettier": "error",
    "react-hooks/rules-of-hooks": "error",
    "react/jsx-one-expression-per-line": [0, "never"],
    "react/prop-types": [0, "never"]
  },
  "env": {
    "browser": true
  }
}